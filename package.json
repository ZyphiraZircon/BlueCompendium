{
  "name": "bluecompendium",
  "version": "1.0.0",
  "description": "Simple FFXIV tools for complex FFXIV problems",
  "main": "index.js",
  "engines": {
    "npm": "3.10.10"
  },
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch ",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zyphxion/BlueCompendium.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zyphxion/BlueCompendium/issues"
  },
  "homepage": "https://github.com/Zyphxion/BlueCompendium#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.9",
    "@fortawesome/free-solid-svg-icons": "^5.6.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-airbnb": "2.2.3",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "express-static-gzip": "^1.1.3",
    "force-ssl-heroku": "^1.0.2",
    "i": "^0.3.6",
    "npm": "^6.4.1",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "6.3.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.23.0",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "compression": "1.6.2",
    "compression-webpack-plugin": "0.4.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "^1.0.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.1.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "jsdom": "9.11.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mocha": "3.2.0",
    "npm-run-all": "^4.1.5",
    "open": "0.0.5",
    "style-loader": "^0.23.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.10.0",
    "webpack-hot-middleware": "2.16.1",
    "webpack-manifest-plugin": "1.1.2"
  }
}
